# Supabase Configuration
# Supabase projenizin URL'sini buraya ekleyin (https://your-project.supabase.co)
VITE_PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# Supabase anon key'inizi buraya ekleyin (Settings > API > anon public)
VITE_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:8080/api

# Multi-Tenant Configuration
VITE_MAIN_DOMAIN=sitem.com
VITE_ENABLE_SUBDOMAINS=true
VITE_TENANT_MODE=subdomain-path
VITE_DEFAULT_SECTOR=dentist

# Supabase tenant tables
VITE_TENANT_TABLE=tenants
VITE_TENANT_SETTINGS_TABLE=tenant_settings

# App Configuration
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0

# Security (32+ character encryption key recommended)
VITE_ENCRYPTION_KEY=your-32-character-encryption-key-change-this-in-production

# Development
NODE_ENV=development
